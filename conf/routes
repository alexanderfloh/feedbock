# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           			controllers.Application.index

GET		/details/:suite/:clazz/:test			controllers.Application.viewDetails(suite: String, clazz: String, test: String)

GET		/loadBuild/:buildNumber						controllers.Application.loadBuild(buildNumber: Int)

POST	/submit									controllers.Application.submit

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               			controllers.Assets.at(path="/public", file)
