# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           			controllers.Application.index

GET		/detailsById/:id							controllers.Application.viewDetailsById(id: String)

GET		/details/:suite/:clazz/:test			controllers.Application.viewDetails(suite: String, clazz: String, test: String)

GET		/load									controllers.Application.load
GET		/loadBuild/:buildNumber						controllers.Application.loadBuild(buildNumber: Int)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               			controllers.Assets.at(path="/public", file)
